{
  "repositories": [
    {
      "url": "https://github.com/lotharthesavior/BagItPHP.git",
      "type": "vcs"
    }
  ],
  "require": {
    "zendframework/zend-diactoros": "^1.3",
    "league/flysystem": "^1.0",
    "league/oauth2-server": "^5.1",
    "slim/slim": "^3.7.0",
    "lotharthesavior/bagit": "dev-master",
    "guzzlehttp/guzzle": "~6.0",
    "league/flysystem-cached-adapter": "^1.0",
    "php-ds/php-ds": "^1.2",
    "lotharthesavior/branch": "^1",
    "pachico/slim-swoole": "^0.3.0",
    "league/plates": "^3.3",
    "monolog/monolog": "^2.2",
    "ext-json": "*",
    "psy/psysh": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Models\\": "src/Models/",
      "Controllers\\": "src/Controllers/",
      "Repositories\\": "src/Repositories/",
      "Helpers\\": "src/Helpers/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "symfony/var-dumper": "^5.0"
  }
}
